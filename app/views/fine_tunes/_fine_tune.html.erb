<tr class="border-t border-neutral" id="fine_tune_<%= fine_tune.id %>">
  <td class="text-white">
    <%= fine_tune.name %>
  </td>
  <td class="text-white">
    <%= fine_tune.base_model&.name %>
  </td>
  <td class="flex space-x-2 text-white">
    <%= link_to "Edit", edit_fine_tune_path(fine_tune),
              class: "text-blue-500 hover:underline",
              data: { turbo_stream: true } %>
    <%= button_to "Delete", fine_tune_path(fine_tune),
                method: :delete,
                class: "text-red-500 hover:underline" %>
  </td>
</tr>